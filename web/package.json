  "homepage": "https://github.com/stencila/stencila/tree/main/web#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/stencila.git"
  },
  "keywords": [
    "types",
    "programmable",
    "executable",
    "reproducible",
    "interactive",
    "documents"
