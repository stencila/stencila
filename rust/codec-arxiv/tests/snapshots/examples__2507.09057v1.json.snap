                },
                {
                  "type": "MathInline",
                  "code": {
                    "string": "g"
                  },
                  "mathLanguage": "latex",
                  "mathml": "redacted"
                },
                {
                  "type": "Text",
                  "value": {
                    "string": " on "
                  }
                },
                {
                  "type": "MathInline",
                  "code": {
                    "string": "[-1,1]"
