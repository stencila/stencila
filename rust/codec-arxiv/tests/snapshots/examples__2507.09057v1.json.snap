          "type": "MathBlock",
          "code": {
            "string": "\\displaystyle\\bm{Q}_{b}"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        }
      ],
      "sectionType": "Main"
    },
    {
      "type": "Section",
      "content": [
        {
          "type": "Heading",
          "label": "C.4",
          "level": 2,
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Covariance matrix of random effects"
              }
            }
          ]
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "The conditional posterior of "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\Sigma_{b}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is an inverse-Wishart distribution, "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\mathcal{IW}\\left(c+n,S+\\sum_{i=1}^{n}\\bm{b}_{i}\\bm{b}_{i}^{\\top}\\right)"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "c=m/2+2"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " and "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "S=(E_{W}-\\rho W)^{-1}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", as defined in Section "
              }
            },
            {
              "type": "Link",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "2.3.3"
                  }
                }
              ],
              "target": "S2.SS3.SSS3",
              "title": "2.3.3 Prior for the covariance matrix of the spatial random effects ‣ 2.3 Bayesian inference ‣ 2 Statistical methodology ‣ A monotone single index model for spatially-referenced multistate current status data"
            },
            {
              "type": "Text",
              "value": {
                "string": " of the main document."
              }
            }
          ]
        }
      ],
      "sectionType": "Main"
    },
    {
      "type": "Section",
      "content": [
        {
          "type": "Heading",
          "label": "C.5",
          "level": 2,
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Density of errors"
              }
            }
          ]
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "The stick-breaking construction "
              }
            },
            {
              "type": "Citation",
              "target": "bib.bib34",
              "citationMode": "Parenthetical",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Sethuraman, 1994"
                  }
                }
              ]
            },
            {
              "type": "Text",
              "value": {
                "string": " allows us to express "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "G"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " in ("
              }
            },
            {
              "type": "Link",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "C.1"
                  }
                }
              ],
              "target": "A3.E1",
              "title": "In Appendix C Posterior Computation ‣ A monotone single index model for spatially-referenced multistate current status data"
            },
            {
              "type": "Text",
              "value": {
                "string": ") as"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "\\displaystyle G(\\mu,\\sigma^{2})"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "where GEM stands for Griffiths, Engen and McCloskey "
              }
            },
            {
              "type": "Citation",
              "target": "bib.bib30",
              "citationMode": "Parenthetical",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Pitman, 2002"
                  }
                }
              ]
            },
            {
              "type": "Text",
              "value": {
                "string": ", and "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\delta_{(\\phi,\\,s^{2})}(\\cdot)"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is the Dirac measure at "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "(\\phi,s^{2})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". Here, "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "(\\phi_{h},s_{h}^{2})_{h=1}^{\\infty}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " denote the distinct values of the atoms "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "(\\mu_{ij},\\sigma_{ij})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " used in ("
              }
            },
            {
              "type": "Link",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "C.1"
                  }
                }
              ],
              "target": "A3.E1",
              "title": "In Appendix C Posterior Computation ‣ A monotone single index model for spatially-referenced multistate current status data"
            },
            {
              "type": "Text",
              "value": {
                "string": "). Posterior sampling for the parameters of the DP mixture prior on the baseline density "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "f"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is performed using a blocked Gibbs algorithm by "
              }
            },
            {
              "type": "Citation",
              "target": "bib.bib20",
              "citationMode": "Narrative",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Ishwaran and James, (2001)"
                  }
                }
              ]
            },
            {
              "type": "Text",
              "value": {
                "string": " that uses a finite truncation for the number of mixture components upto a large number H."
              }
            }
          ]
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Recall that "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\epsilon_{ij}=\\text{log }T_{ij}-b_{ij}-g(\\bm{x}_{ij}^{\\top}\\mbox{\\boldmath$%\n\\beta$})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", for each "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "i"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "j"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". The hierarchical representation of a DP mixture prior of Gaussians on "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\epsilon_{ij}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " with a normal-inverse gamma base distribution "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "G_{0}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " and a gamma prior on the concentration parameter "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\gamma"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is as follows:"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "\\displaystyle\\epsilon_{ij}\\mid z_{ij},\\mbox{\\boldmath$\\phi$},\\bm{s}"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\mbox{\\boldmath$\\phi$}=(\\phi_{1},\\phi_{2},\\ldots,\\phi_{H})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\bm{s}=(s^{2}_{1},s^{2}_{2},\\ldots,s^{2}_{H})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " and "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\mbox{\\boldmath$\\pi$}=(\\pi_{1},\\pi_{2},\\ldots,\\pi_{H})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". Note that the prior on "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\pi"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " can be explicitly written as "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\pi_{1}=\\pi_{1}^{\\prime}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\pi_{h}=\\pi^{\\prime}_{h}\\prod_{l&lt;h}(1-\\pi^{\\prime}_{l})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " for "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "1&lt;h&lt;H"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", and "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\pi_{H}=\\prod_{h=1}^{H-1}(1-\\pi_{h})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\pi_{l}^{\\prime}\\mid\\gamma\\sim\\text{Beta}(1,\\gamma)"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "l=1,\\ldots,H-1"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". The atoms "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "(\\mu_{ij},\\sigma_{ij}^{2})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " can be obtained using the distinct atoms "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "(\\phi_{h},s_{h}^{2})_{h=1}^{H}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " and the cluster labels "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "z_{ij}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " as follows,"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "\\mu_{ij}=\\phi_{z_{ij}},\\quad{and}\\quad\\sigma_{ij}^{2}=s_{z_{ij}}^{2},\\quad%\n\\text{for each }i,j."
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Blocked posterior updates of the parameters are as follows:"
              }
            }
          ]
        },
        {
          "type": "List",
          "items": [
            {
              "type": "ListItem",
              "content": [
                {
                  "type": "Paragraph",
                  "content": []
                },
                {
                  "type": "Paragraph",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "Update "
                      }
                    },
                    {
                      "type": "MathInline",
                      "code": {
                        "string": "\\bm{z}"
                      },
                      "mathLanguage": "latex",
                      "mathml": "<math alttext=\"\\bm{z}\" display=\"inline\" id=\"A3.I2.i1.p1.1.m1.1\" class=\"ltx_Math\"><semantics id=\"A3.I2.i1.p1.1.m1.1a\"><mi xref=\"A3.I2.i1.p1.1.m1.1.1.cmml\" id=\"A3.I2.i1.p1.1.m1.1.1\">𝒛</mi><annotation-xml encoding=\"MathML-Content\" id=\"A3.I2.i1.p1.1.m1.1b\"><ci xref=\"A3.I2.i1.p1.1.m1.1.1\" id=\"A3.I2.i1.p1.1.m1.1.1.cmml\">𝒛</ci></annotation-xml><annotation encoding=\"application/x-tex\" id=\"A3.I2.i1.p1.1.m1.1c\">\\bm{z}</annotation><annotation encoding=\"application/x-llamapun\" id=\"A3.I2.i1.p1.1.m1.1d\">bold_italic_z</annotation></semantics></math>"
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": ". For each "
                      }
                    },
                    {
                      "type": "MathInline",
                      "code": {
                        "string": "i"
                      },
                      "mathLanguage": "latex",
                      "mathml": "<math alttext=\"i\" display=\"inline\" id=\"A3.I2.i1.p1.2.m2.1\" class=\"ltx_Math\"><semantics id=\"A3.I2.i1.p1.2.m2.1a\"><mi xref=\"A3.I2.i1.p1.2.m2.1.1.cmml\" id=\"A3.I2.i1.p1.2.m2.1.1\">i</mi><annotation-xml encoding=\"MathML-Content\" id=\"A3.I2.i1.p1.2.m2.1b\"><ci xref=\"A3.I2.i1.p1.2.m2.1.1\" id=\"A3.I2.i1.p1.2.m2.1.1.cmml\">𝑖</ci></annotation-xml><annotation encoding=\"application/x-tex\" id=\"A3.I2.i1.p1.2.m2.1c\">i</annotation><annotation encoding=\"application/x-llamapun\" id=\"A3.I2.i1.p1.2.m2.1d\">italic_i</annotation></semantics></math>"
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": ", "
                      }
                    },
                    {
                      "type": "MathInline",
                      "code": {
                        "string": "j"
                      },
                      "mathLanguage": "latex",
                      "mathml": "<math alttext=\"j\" display=\"inline\" id=\"A3.I2.i1.p1.3.m3.1\" class=\"ltx_Math\"><semantics id=\"A3.I2.i1.p1.3.m3.1a\"><mi xref=\"A3.I2.i1.p1.3.m3.1.1.cmml\" id=\"A3.I2.i1.p1.3.m3.1.1\">j</mi><annotation-xml encoding=\"MathML-Content\" id=\"A3.I2.i1.p1.3.m3.1b\"><ci xref=\"A3.I2.i1.p1.3.m3.1.1\" id=\"A3.I2.i1.p1.3.m3.1.1.cmml\">𝑗</ci></annotation-xml><annotation encoding=\"application/x-tex\" id=\"A3.I2.i1.p1.3.m3.1c\">j</annotation><annotation encoding=\"application/x-llamapun\" id=\"A3.I2.i1.p1.3.m3.1d\">italic_j</annotation></semantics></math>"
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": ","
                      }
                    }
                  ]
                },
                {
                  "type": "MathBlock",
                  "code": {
                    "string": "z_{ij}\\mid\\ldots\\sim\\text{Cat}\\,(\\pi^{*}_{1},\\ldots,\\pi^{*}_{H}),"
                  },
