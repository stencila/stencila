                {
                  "type": "MathInline",
                  "code": {
                    "string": "\\mathcal{O}(L^{2})"
                  },
                  "mathLanguage": "latex",
                  "mathml": "redacted"
                },
                {
                  "type": "Text",
                  "value": {
                    "string": " computation for inverting and storing the covariance matrix, posing challenges to the sampling algorithm for large "
                  }
                },
                {
                  "type": "MathInline",
                  "code": {
                    "string": "L"
                  },
                  "mathLanguage": "latex",
                  "mathml": "redacted"
                },
                {
                  "type": "Text",
                  "value": {
                    "string": ". Deviating slightly from the formulation of "
                  }
                },
                {
                  "type": "Citation",
                  "target": "bib.bib25",
                  "citationMode": "Narrative",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "Maatouk and Bay, (2017)"
                      }
                    }
                  ]
                },
                {
                  "type": "Text",
                  "value": {
                    "string": " and following the specification of "
                  }
                },
                {
                  "type": "Citation",
                  "target": "bib.bib32",
                  "citationMode": "Narrative",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "Ray et al., (2020)"
                      }
                    }
                  ]
                },
                {
                  "type": "Text",
                  "value": {
                    "string": ", we put a stationary GP on the first derivative of "
                  }
                },
                {
                  "type": "MathInline",
                  "code": {
                    "string": "g"
                  },
                  "mathLanguage": "latex",
                  "mathml": "redacted"
                },
                {
                  "type": "Text",
                  "value": {
                    "string": ","
                  }
                }
              ]
            },
            {
              "type": "MathBlock",
              "code": {
                "string": "p(\\mbox{\\boldmath$\\xi$})\\propto\\mathcal{N}(0,\\tau^{2}K)\\ \\mathds{1}_{\\mathcal{%\nC}_{\\xi}}(\\mbox{\\boldmath$\\xi$}),"
