                },
                {
                  "type": "MathInline",
                  "code": {
                    "string": "\\ell&gt;0"
                  },
                  "mathLanguage": "latex",
                  "mathml": "redacted"
                },
                {
                  "type": "Text",
                  "value": {
                    "string": ". is placed on "
