      "content": [
        {
          "type": "Strong",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Step 3."
              }
            }
          ]
        },
        {
          "type": "Text",
          "value": {
            "string": " Generate the random effects "
          }
        },
        {
          "type": "MathInline",
          "code": {
            "string": "\\bm{b}_{i}=(b_{i1},\\ldots,b_{i\\,m/2})^{\\top}\\sim\\mathcal{N}(0,\\Sigma_{b})"
