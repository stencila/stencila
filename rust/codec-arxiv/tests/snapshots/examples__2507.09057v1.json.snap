        {
          "type": "MathInline",
          "code": {
            "string": "\\mbox{\\boldmath$\\beta$}^{0}=\\widetilde{}\\mbox{\\boldmath$\\beta$}^{0}\\,/\\,\\|%\n\\widetilde{}\\mbox{\\boldmath$\\beta$}^{0}\\|"
