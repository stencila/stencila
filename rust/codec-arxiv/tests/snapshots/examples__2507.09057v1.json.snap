    },
    {
      "type": "Section",
      "content": [
        {
          "type": "Heading",
          "label": "C.1",
          "level": 2,
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Regression parameters"
              }
            }
          ]
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Define"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "u_{ij}=\\text{log }T_{ij}-b_{ij}-\\mu_{ij},\\quad\\text{for each }i,j."
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "The conditional posterior of "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\widetilde{}\\mbox{\\boldmath$\\beta$}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is given by, "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "p\\big{(}\\,\\widetilde{}\\mbox{\\boldmath$\\beta$}\\mid\\ldots\\,\\big{)}\\propto\\ %\n\\mathcal{N}\\big{(}\\widetilde{}\\mbox{\\boldmath$\\beta$}\\,;0,\\sigma_{\\beta}^{2}%\n\\big{)}\\ L_{\\beta}\\big{(}\\,\\widetilde{}\\mbox{\\boldmath$\\beta$}\\,\\big{)},"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " where,"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "\\text{log }L_{\\beta}\\big{(}\\,\\widetilde{}\\mbox{\\boldmath$\\beta$}\\,\\big{)}=-%\n\\frac{1}{2}\\sum_{i,j}\\frac{1}{\\sigma_{ij}^{2}}\\left(u_{ij}-\\sum_{l=0}^{L}\\xi_{%\nl}\\,\\psi_{l}\\big{(}\\bm{x}_{ij}^{\\top}\\frac{\\widetilde{}\\mbox{\\boldmath$\\beta$}%\n}{\\|\\widetilde{}\\mbox{\\boldmath$\\beta$}\\|}\\big{)}\\right)^{2}."
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "MathInline",
              "code": {
                "string": "\\widetilde{}\\mbox{\\boldmath$\\beta$}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is sampled using the elliptical slice sampler "
              }
            },
            {
              "type": "Citation",
              "target": "bib.bib29",
              "citationMode": "Parenthetical",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "ESS, Murray et al., 2010"
                  }
                }
              ]
            },
            {
              "type": "Text",
              "value": {
                "string": ", particularly suited for posterior sampling in models with multivariate Gaussian priors."
              }
            }
          ]
        }
      ],
      "sectionType": "Main"
    },
    {
      "type": "Section",
      "content": [
        {
          "type": "Heading",
          "label": "C.2",
          "level": 2,
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Monotone link function"
              }
            }
          ]
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Let "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "u_{ij}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " be defined as ("
              }
            },
            {
              "type": "Link",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "C.2"
                  }
                }
              ],
              "target": "A3.E2",
              "title": "In C.1 Regression parameters ‣ Appendix C Posterior Computation ‣ A monotone single index model for spatially-referenced multistate current status data"
            },
            {
              "type": "Text",
              "value": {
                "string": "). Define "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\Psi"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " be an "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "N\\times(L+1)"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " matrix with elements "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\Psi_{rl}=\\psi_{l}(\\mbox{\\boldmath$\\beta$}^{T}\\bm{x}^{*}_{r})/\\sigma_{r}^{*}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "N=mn"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", and"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "\\displaystyle(\\bm{x}^{*}_{1},\\ldots,\\bm{x}^{*}_{N})"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "are simply the stacked covariates and error standard deviations over "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "i"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " and "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "j"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". Similarly stack "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\bm{u}=(u_{11},u_{12},\\ldots,u_{1m},\\ldots,u_{n1},u_{n2},\\ldots,u_{nm})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": "."
              }
            }
          ]
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "The coefficients "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\xi"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " in the basis expansion of the monotone link function given in equation ("
              }
            },
            {
              "type": "Link",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "2.7"
                  }
                }
              ],
              "target": "S2.E7",
              "title": "In 2.3.2 Model and prior for the monotone link function ‣ 2.3 Bayesian inference ‣ 2 Statistical methodology ‣ A monotone single index model for spatially-referenced multistate current status data"
            },
            {
              "type": "Text",
              "value": {
                "string": ") of the main document, have a zero-mean Gaussian prior with covariance matrix "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\tau^{2}K"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "K_{ij}=k(u_{i}-u_{j})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "k(\\cdot)"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is the stationary Matérn covariance kernel with smoothness parameter "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\nu&gt;0"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " and length-scale parameter "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\ell&gt;0"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". The most general definition of the Matérn covariance kernel is the following:"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "k(r)=\\frac{2^{1-\\nu}}{\\Gamma(\\nu)}\\left(\\frac{\\sqrt{2\\nu}\\,r}{\\ell}\\right)^{%\n\\nu}K_{\\nu}\\left(\\frac{\\sqrt{2\\nu}\\,r}{\\ell}\\right),"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "K_{\\nu}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " is the modified Bessel function of the second kind. The parameter "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\tau"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " controls the prior signal to noise ratio. A non-informative inverse-gamma "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\mathcal{IG}(a_{\\xi},b_{\\xi})"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " prior is placed on "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\tau^{2}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". The conditional posterior of the parameters involving estimation of the monotone link function is given by,"
              }
            }
          ]
        },
        {
          "type": "MathBlock",
          "code": {
            "string": "\\displaystyle p(\\bm{\\xi},\\tau\\mid\\ldots)\\propto(\\tau^{2})^{-(a_{\\xi}+1)}e^{-{b%\n_{\\xi}}/\\tau^{2}}(\\tau^{2})^{-(L+1)/2}e^{\\,-{\\bm{\\xi}}^{\\top}K^{-1}{\\bm{\\xi}}%\n\\,/\\,(2\\tau^{2})}\\ \\mathbb{J}_{\\eta}(\\bm{\\xi})\\ e^{\\,-\\|\\bm{u}-\\Psi{\\bm{\\xi}}%\n\\|^{2}/2},"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "where "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\mathbb{J}_{\\eta}(\\mbox{\\boldmath$\\xi$})=\\prod_{l=0}^{L}(1+e^{\\,-\\eta\\,\\xi_{l}%\n})^{-1}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". The sampling algorithm proceeds as follows:"
              }
            }
          ]
        },
        {
          "type": "List",
          "items": [
            {
              "type": "ListItem",
              "content": [
                {
                  "type": "Paragraph",
                  "content": []
                },
                {
                  "type": "Paragraph",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "Update "
                      }
                    },
                    {
                      "type": "MathInline",
                      "code": {
                        "string": "\\xi"
                      },
                      "mathLanguage": "latex",
                      "mathml": "<math alttext=\"\\xi\" display=\"inline\" id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1\" class=\"ltx_Math\"><semantics id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1a\"><mi xref=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1.1.cmml\" id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1.1\">𝝃</mi><annotation-xml encoding=\"MathML-Content\" id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1b\"><ci xref=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1.1\" id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1.1.cmml\">𝝃</ci></annotation-xml><annotation encoding=\"application/x-tex\" id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1c\">\\xi</annotation><annotation encoding=\"application/x-llamapun\" id=\"A3.I1.i1.p1.1.m1.1.1.1.m1.1d\">bold_italic_ξ</annotation></semantics></math>"
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": ": The full conditional of "
                      }
                    },
                    {
                      "type": "MathInline",
                      "code": {
                        "string": "\\xi"
                      },
                      "mathLanguage": "latex",
                      "mathml": "<math alttext=\"\\xi\" display=\"inline\" id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1\" class=\"ltx_Math\"><semantics id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1a\"><mi xref=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1.1.cmml\" id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1.1\">𝝃</mi><annotation-xml encoding=\"MathML-Content\" id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1b\"><ci xref=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1.1\" id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1.1.cmml\">𝝃</ci></annotation-xml><annotation encoding=\"application/x-tex\" id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1c\">\\xi</annotation><annotation encoding=\"application/x-llamapun\" id=\"A3.I1.i1.p1.2.m2.1.1.1.m1.1d\">bold_italic_ξ</annotation></semantics></math>"
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": " is given by"
                      }
                    }
                  ]
                },
                {
                  "type": "MathBlock",
                  "code": {
                    "string": "p(\\mbox{\\boldmath$\\xi$}\\mid\\ldots\\,)\\propto\\ \\mathcal{N}(\\mbox{\\boldmath$\\xi$}%\n\\,;0,\\tau^{2}K)\\ L_{\\xi}(\\mbox{\\boldmath$\\xi$}),"
                  },
