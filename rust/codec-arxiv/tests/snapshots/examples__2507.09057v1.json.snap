        {
          "type": "MathInline",
          "code": {
            "string": "100"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Text",
          "value": {
            "string": ". More details on the choice hyperparameters for each prior are provided in Section "
          }
        },
