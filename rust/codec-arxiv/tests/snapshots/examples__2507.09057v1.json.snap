        },
        {
          "type": "MathInline",
          "code": {
            "string": "\\Sigma_{b}=0.1^{2}(E_{W}-0.9W)^{-1}"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Text",
          "value": {
            "string": " "
          }
        },
        {
          "type": "Emphasis",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "i.e."
              }
            }
          ]
        },
        {
          "type": "Text",
          "value": {
            "string": "from a parametric CAR density, with "
          }
