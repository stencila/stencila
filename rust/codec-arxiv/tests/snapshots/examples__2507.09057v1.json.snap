                  "mathml": "<math alttext=\"T_{i^{\\prime}}=\\exp{\\{Y_{i^{\\prime}}\\}}\" display=\"inline\" id=\"A4.I1.ix3.p1.1.m1.2\" class=\"ltx_Math\"><semantics id=\"A4.I1.ix3.p1.1.m1.2a\"><mrow xref=\"A4.I1.ix3.p1.1.m1.2.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2\"><msub xref=\"A4.I1.ix3.p1.1.m1.2.2.3.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.3\"><mi xref=\"A4.I1.ix3.p1.1.m1.2.2.3.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.2\">T</mi><msup xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3\"><mi xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3.2\">i</mi><mo xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3.3.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3.3\">′</mo></msup></msub><mo xref=\"A4.I1.ix3.p1.1.m1.2.2.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.2\">=</mo><mrow xref=\"A4.I1.ix3.p1.1.m1.2.2.1.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1\"><mi xref=\"A4.I1.ix3.p1.1.m1.1.1.cmml\" id=\"A4.I1.ix3.p1.1.m1.1.1\">exp</mi><mo xref=\"A4.I1.ix3.p1.1.m1.2.2.1.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1a\">⁡</mo><mrow xref=\"A4.I1.ix3.p1.1.m1.2.2.1.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1\"><mo stretchy=\"false\" xref=\"A4.I1.ix3.p1.1.m1.2.2.1.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.2\">{</mo><msub xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1\"><mi xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.2\">Y</mi><msup xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3\"><mi xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.2\">i</mi><mo xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.3.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.3\">′</mo></msup></msub><mo stretchy=\"false\" xref=\"A4.I1.ix3.p1.1.m1.2.2.1.2.cmml\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.3\">}</mo></mrow></mrow></mrow><annotation-xml encoding=\"MathML-Content\" id=\"A4.I1.ix3.p1.1.m1.2b\"><apply xref=\"A4.I1.ix3.p1.1.m1.2.2\" id=\"A4.I1.ix3.p1.1.m1.2.2.cmml\"><eq xref=\"A4.I1.ix3.p1.1.m1.2.2.2\" id=\"A4.I1.ix3.p1.1.m1.2.2.2.cmml\"></eq><apply xref=\"A4.I1.ix3.p1.1.m1.2.2.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.cmml\"><csymbol cd=\"ambiguous\" xref=\"A4.I1.ix3.p1.1.m1.2.2.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.1.cmml\">subscript</csymbol><ci xref=\"A4.I1.ix3.p1.1.m1.2.2.3.2\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.2.cmml\">𝑇</ci><apply xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3.cmml\"><csymbol cd=\"ambiguous\" xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3.1.cmml\">superscript</csymbol><ci xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3.2\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3.2.cmml\">𝑖</ci><ci xref=\"A4.I1.ix3.p1.1.m1.2.2.3.3.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.3.3.3.cmml\">′</ci></apply></apply><apply xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.2.cmml\"><exp xref=\"A4.I1.ix3.p1.1.m1.1.1\" id=\"A4.I1.ix3.p1.1.m1.1.1.cmml\"></exp><apply xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.cmml\"><csymbol cd=\"ambiguous\" xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.1.cmml\">subscript</csymbol><ci xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.2\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.2.cmml\">𝑌</ci><apply xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.cmml\"><csymbol cd=\"ambiguous\" xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.1.cmml\">superscript</csymbol><ci xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.2\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.2.cmml\">𝑖</ci><ci xref=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.3\" id=\"A4.I1.ix3.p1.1.m1.2.2.1.1.1.1.3.3.cmml\">′</ci></apply></apply></apply></apply></annotation-xml><annotation encoding=\"application/x-tex\" id=\"A4.I1.ix3.p1.1.m1.2c\">T_{i^{\\prime}}=\\exp{\\{Y_{i^{\\prime}}\\}}</annotation><annotation encoding=\"application/x-llamapun\" id=\"A4.I1.ix3.p1.1.m1.2d\">italic_T start_POSTSUBSCRIPT italic_i start_POSTSUPERSCRIPT ′ end_POSTSUPERSCRIPT end_POSTSUBSCRIPT = roman_exp { italic_Y start_POSTSUBSCRIPT italic_i start