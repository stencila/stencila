                {
                  "type": "MathInline",
                  "code": {
                    "string": "\\mathds{1}_{\\mathcal{C}_{\\xi}}(\\mbox{\\boldmath$\\xi$})\\approx\\mathbb{J}_{\\eta}(%\n\\mbox{\\boldmath$\\xi$})=\\prod_{l=0}^{L}(1+e^{\\,-\\eta\\,\\xi_{l}})^{-1}"
