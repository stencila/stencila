        {
          "type": "MathInline",
          "code": {
            "string": "i"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Text",
          "value": {
            "string": " and "
          }
        },
        {
          "type": "MathInline",
          "code": {
            "string": "j"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Text",
          "value": {
            "string": ". Here "
          }
        },
        {
          "type": "MathInline",
          "code": {
            "string": "\\widetilde{x}_{ij}^{\\,(1)}"
          },
          "mathLanguage": "latex",
          "mathml": "redacted"
        },
        {
          "type": "Text",
          "value": {
