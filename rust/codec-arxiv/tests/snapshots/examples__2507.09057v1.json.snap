                },
                {
                  "type": "MathInline",
                  "code": {
                    "string": "\\nu&gt;0"
                  },
                  "mathLanguage": "latex",
                  "mathml": "redacted"
                },
                {
                  "type": "Text",
                  "value": {
                    "string": " and length-scale "
