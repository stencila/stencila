    {
      "type": "Claim",
      "claimType": "Theorem",
      "label": "A.4",
      "content": [
        {
          "type": "Paragraph",
          "content": [
            {
              "type": "Text",
              "value": {
                "string": "Assume the same setting as in Example "
              }
            },
            {
              "type": "Link",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "A.2"
                  }
                }
              ],
              "target": "A1.Thmtheorem2",
              "title": "Example A.2 (A ùúé-algebra for the Boolean interpretations of propositional logic). ‚Ä£ Appendix A Basic of measure theory ‚Ä£ Defining neurosymbolic AI"
            },
            {
              "type": "Text",
              "value": {
                "string": " and take the measurable space "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "(\\Omega,\\mathcal{P}(\\Omega))"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " with "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\Omega=\\mathbb{B}^{26}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ". A well-known measure for finite measurable spaces is the "
              }
            },
            {
              "type": "Emphasis",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "counting measure"
                  }
                }
              ]
            },
            {
              "type": "Text",
              "value": {
                "string": " "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "m_{C}"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": " that outputs the cardinality of each element of "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "\\Sigma"
              },
              "mathLanguage": "latex",
              "mathml": "redacted"
            },
            {
              "type": "Text",
              "value": {
                "string": ", i.e. "
              }
            },
            {
              "type": "MathInline",
              "code": {
                "string": "m_{C}(S)=\\left|S\\right|"
