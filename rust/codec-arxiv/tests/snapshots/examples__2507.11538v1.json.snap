                    },
                    {
                      "type": "MathInline",
                      "code": {
                        "string": "&lt;"
                      },
                      "mathLanguage": "latex",
                      "mathml": "redacted"
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": "20 words), or incoherent reports (validated by a secondary "
                      }
                    },
                    {
                      "type": "CodeInline",
                      "code": {
                        "string": "o4-mini"
                      }
                    },
                    {
                      "type": "Text",
                      "value": {
