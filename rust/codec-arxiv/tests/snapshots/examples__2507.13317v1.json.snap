            {
              "type": "Citation",
              "target": "bib.bib30",
              "citationMode": "Parenthetical",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Ettori et al., 2009"
                  }
                }
              ]
            },
            {
              "type": "Citation",
              "target": "bib.bib9",
              "citationMode": "Parenthetical",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Arnaud et al., 2011"
                  }
                }
              ]
            },
            {
              "type": "Citation",
              "target": "bib.bib38",
              "citationMode": "Parenthetical",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Ghirardini et al., 2024"
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "Text",
          "value": {
            "string": ". These observations probe the integrated temperature and density of the gas and provide crucial information about the impact of feedback processes "
          }
        },
        {
          "type": "CitationGroup",
          "items": [
            {
              "type": "Citation",
              "target": "bib.bib98",
              "citationMode": "Parenthetical",
              "content": [
