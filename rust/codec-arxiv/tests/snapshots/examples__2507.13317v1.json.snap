        },
        {
          "type": "Section",
          "sectionType": "Main",
          "content": [
            {
              "type": "Heading",
              "label": "6.1.1",
              "level": 3,
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "Six-parameter set"
                  }
                }
              ]
            },
            {
              "type": "Paragraph",
              "content": [
                {
                  "type": "Text",
                  "value": {
                    "string": "We first jointly fit the matter–matter and matter–pressure response using a minimal version of each model which comprises only six free parameters, these are:"
                  }
                }
              ]
            },
            {
              "type": "List",
              "items": [
                {
                  "type": "ListItem",
                  "content": [
                    {
                      "type": "Paragraph",
                      "content": []
                    },
                    {
                      "type": "Paragraph",
                      "content": [
                        {
                          "type": "Text",
                          "value": {
                            "string": "Mead20+: "
                          }
                        },
                        {
                          "type": "MathInline",
                          "code": {
                            "string": "\\epsilon_{1},\\,\\Gamma,\\,\\log M_{0},\\,\\alpha,\\,\\log T_{\\rm w},\\,\\alpha_{\\rm\nsmooth%\n,mp}"
                          },
                          "mathLanguage": "latex",
                          "mathml": "redacted"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "ListItem",
                  "content": [
                    {
                      "type": "Paragraph",
                      "content": []
                    },
                    {
                      "type": "Paragraph",
                      "content": [
                        {
                          "type": "Text",
                          "value": {
                            "string": "Schneider19+: "
                          }
                        },
