            {
              "type": "MathBlock",
              "id": "inz212599-disp-0003",
              "code": {
                "string": "<math id=\"jats-math-3\" display=\"block\">\n              <mrow>\n                <mrow>\n                  <mi>k</mi>\n                  <msub>\n                    <mi>S</mi>\n                    <mi mathvariant=\"normal\">B</mi>\n                  </msub>\n                  <mo linebreak=\"badbreak\">=</mo>\n                  <msub>\n                    <mi>S</mi>\n                    <mi mathvariant=\"normal\">B</mi>\n                  </msub>\n                  <mi mathvariant=\"normal\">\n                    Δ\n                  </mi>\n                  <msub>\n                    <mi>h</mi>\n                    <mi mathvariant=\"normal\">B</mi>\n                  </msub>\n                  <mi>\n                    ρ\n                  </mi>\n                  <mi>g</mi>\n                </mrow>\n              </mrow>\n            </math>"
              },
              "mathLanguage": "mathml",
              "label": "3",
              "labelAutomatically": false
            },
            {
              "type": "Paragraph",
              "content": [
                {
                  "type": "Text",
