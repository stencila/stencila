                }
              ]
            },
            {
              "type": "MathBlock",
              "id": "inz212599-disp-0001",
              "code": {
                "string": "<math id=\"jats-math-1\" display=\"block\">\n              <mrow>\n                <mrow>\n                  <mi>k</mi>\n                  <msub>\n                    <mi>S</mi>\n                    <mi mathvariant=\"normal\">A</mi>\n                  </msub>\n                  <mo linebreak=\"badbreak\">=</mo>\n                  <msub>\n                    <mi>m</mi>\n                    <mi mathvariant=\"normal\">A</mi>\n                  </msub>\n                  <mi>g</mi>\n                  <mo linebreak=\"goodbreak\">=</mo>\n                  <msub>\n                    <mi>V</mi>\n                    <mi mathvariant=\"normal\">A</mi>\n                  </msub>\n                  <mi>\n                    ρ\n                  </mi>\n                  <mi>g</mi>\n                  <mo linebreak=\"goodbreak\">=</mo>\n                  <msub>\n                    <mi>S</mi>\n                    <mi mathvariant=\"normal\">A</mi>\n                  </msub>\n                  <msub>\n                    <mi>h</mi>\n                    <mi mathvariant=\"normal\">A</mi>\n                  </msub>\n                  <mi>\n                    ρ\n                  </mi>\n                  <mi>g</mi>\n                </mrow>\n              </mrow>\n            </math>"
              },
