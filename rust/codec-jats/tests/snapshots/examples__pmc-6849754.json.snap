                      }
                    }
                  ]
                },
                {
                  "type": "Text",
                  "value": {
                    "string": " that were standardized to top of the canopy light values ("
                  }
                },
                {
                  "type": "Emphasis",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "\n          Q"
                      }
                    }
                  ]
                },
                {
                  "type": "Text",
                  "value": {
                    "string": "\n"
                  }
                },
                {
                  "type": "Subscript",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "\n            "
                      }
                    },
                    {
                      "type": "Emphasis",
                      "content": [
                        {
                          "type": "Text",
                          "value": {
                            "string": "int"
                          }
                        }
                      ]
                    },
                    {
                      "type": "Text",
                      "value": {
                        "string": "\n          "
                      }
                    }
                  ]
                },
                {
                  "type": "Text",
                  "value": {
                    "string": " = 40 mol m"
                  }
                },
                {
                  "type": "Superscript",
                  "content": [
                    {
                      "type": "Text",
                      "value": {
                        "string": "−2"
                      }
                    }
