{
  "$schema": "https://stencila.org/Article.schema.json",
  "type": "Article",
  "content": [
    {
      "type": "Paragraph",
      "content": [{ "type": "Text", "value": "A code block" }]
    },
    {
      "type": "CodeBlock",
      "programmingLanguage": "python",
      "code": "# Some python code\na = 3"
    },
    {
      "type": "Paragraph",
      "content": [{ "type": "Text", "value": "A code block with no language" }]
    },
    {
      "type": "CodeBlock",
      "code": "No language"
    },
    {
      "type": "Paragraph",
      "content": [{ "type": "Text", "value": "An empty code block" }]
    },
    {
      "type": "CodeBlock",
      "code": ""
    },
    {
      "type": "Paragraph",
      "content": [
        { "type": "Text", "value": "A code block with only whitespace" }
      ]
    },
    {
      "type": "CodeBlock",
      "code": " \t\n "
    }
  ]
}
