{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Figure",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "ImageObject",
              "contentUrl": "https://example.org/cat.jpg"
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Figure",
      "stencila:label": "2",
      "stencila:labelAutomatically": false,
      "caption": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "A dog."
              }
            }
          ]
        }
      ],
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "ImageObject",
              "contentUrl": "https://example.org/dog.jpg"
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Section",
      "stencila:content": [
        {
          "@type": "stencila:Figure",
          "caption": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": {
                    "string": "A figure of a mouse inside a section."
                  }
                }
              ]
            }
          ],
          "stencila:content": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "ImageObject",
                  "contentUrl": "https://example.org/mouse.jpg"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}