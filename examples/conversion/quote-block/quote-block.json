{
  "$schema": "https://stencila.org/Article.schema.json",
  "type": "Article",
  "content": [
    {
      "type": "Paragraph",
      "content": [{ "type": "Text", "value": "An empty quote block:" }]
    },
    {
      "type": "QuoteBlock",
      "content": []
    },
    {
      "type": "QuoteBlock",
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "This is a quote block" }]
        }
      ]
    },
    {
      "type": "QuoteBlock",
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "This quote block..." }]
        },
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "Has two paragraphs" }]
        }
      ]
    },
    {
      "type": "QuoteBlock",
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "This quote block..." }]
        },
        {
          "type": "QuoteBlock",
          "content": [
            {
              "type": "Paragraph",
              "content": [{ "type": "Text", "value": "Has another inside it." }]
            },
            {
              "type": "QuoteBlock",
              "content": [
                {
                  "type": "Paragraph",
                  "content": [
                    { "type": "Text", "value": "And another inside that!" }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
