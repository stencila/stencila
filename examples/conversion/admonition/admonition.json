{
  "$schema": "https://stencila.org/Article.schema.json",
  "type": "Article",
  "content": [
    {
      "type": "Admonition",
      "admonitionType": "Note",
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "A note" }]
        }
      ]
    },
    {
      "type": "Admonition",
      "admonitionType": "Warning",
      "title": [{ "type": "Text", "value": "With a title" }],
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "A warning" }]
        }
      ]
    },
    {
      "type": "Admonition",
      "admonitionType": "Success",
      "isFolded": false,
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "Foldable, but unfolded" }]
        }
      ]
    },
    {
      "type": "Admonition",
      "admonitionType": "Danger",
      "title": [{ "type": "Text", "value": "Folded with a title" }],
      "isFolded": true,
      "content": [
        {
          "type": "Paragraph",
          "content": [{ "type": "Text", "value": "Foldable" }]
        }
      ]
    },
    {
      "type": "Admonition",
      "admonitionType": "Tip",
      "title": [{ "type": "Text", "value": "Nested admonitions" }],
      "content": [
        {
          "type": "Admonition",
          "admonitionType": "Info",
          "content": [
            {
              "type": "Admonition",
              "admonitionType": "Success",
              "content": [
                {
                  "type": "Paragraph",
                  "content": [
                    {
                      "type": "Text",
                      "value": "This is the first inner-inner admonition"
                    }
                  ]
                }
              ]
            },
            {
              "type": "Admonition",
              "admonitionType": "Error",
              "content": [
                {
                  "type": "Paragraph",
                  "content": [
                    {
                      "type": "Text",
                      "value": "This is the second inner-inner admonition"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
