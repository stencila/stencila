{
  "$schema": "https://stencila.dev/Article.schema.json",
  "type": "Article",
  "content": [
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "With a number output "
        },
        {
          "type": "CodeExpression",
          "programmingLanguage": "python",
          "code": "6 * 7",
          "output": 42
        },
        { "type": "Text", "value": "." }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "With a string output "
        },
        {
          "type": "CodeExpression",
          "programmingLanguage": "python",
          "code": "'a string'",
          "output": "a string"
        },
        { "type": "Text", "value": "." }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "With an array output "
        },
        {
          "type": "CodeExpression",
          "programmingLanguage": "python",
          "code": "[1, 2, 3]",
          "output": [1, 2, 3]
        },
        { "type": "Text", "value": "." }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "With an object output "
        },
        {
          "type": "CodeExpression",
          "programmingLanguage": "python",
          "code": "dict(a=1,b=dict())",
          "output": { "a": 1, "b": {} }
        },
        { "type": "Text", "value": "." }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "With no language specified and no output "
        },
        {
          "type": "CodeExpression",
          "code": "a + b"
        },
        { "type": "Text", "value": "." }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "With execution mode specified and no output "
        },
        {
          "type": "CodeExpression",
          "programmingLanguage": "javascript",
          "code": "c * d",
          "executionMode": "Locked"
        },
        { "type": "Text", "value": "." }
      ]
    }
  ]
}
