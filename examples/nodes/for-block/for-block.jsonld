{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "A simple for loop"
          }
        }
      ]
    },
    {
      "@type": "stencila:ForBlock",
      "stencila:code": {
        "string": "[1,2,3]"
      },
      "stencila:variable": "item",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "This paragraph will be repeated with a different value for "
              }
            },
            {
              "@type": "stencila:CodeExpression",
              "stencila:code": {
                "string": "item"
              }
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With a programming language specified"
          }
        }
      ]
    },
    {
      "@type": "stencila:ForBlock",
      "stencila:code": {
        "string": "[1,2,3]"
      },
      "programmingLanguage": "python",
      "stencila:variable": "item",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "This content is repeated"
              }
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "Nested for loops"
          }
        }
      ]
    },
    {
      "@type": "stencila:ForBlock",
      "stencila:code": {
        "string": "[['a', 'b'], ['c', 'd']]"
      },
      "stencila:variable": "outer",
      "stencila:content": [
        {
          "@type": "stencila:ForBlock",
          "stencila:code": {
            "string": "outer"
          },
          "stencila:variable": "inner",
          "stencila:content": [
            {
              "@type": "stencila:Paragraph",
              "stencila:content": [
                {
                  "@type": "Text",
                  "value": {
                    "string": "This paragraph will be repeated for each inner item "
                  }
                },
                {
                  "@type": "stencila:CodeExpression",
                  "stencila:code": {
                    "string": "inner"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With an else clause"
          }
        }
      ]
    },
    {
      "@type": "stencila:ForBlock",
      "stencila:code": {
        "string": "[]"
      },
      "stencila:variable": "item",
      "stencila:content": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "This content is never shown"
              }
            }
          ]
        }
      ],
      "stencila:otherwise": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "There are no items"
              }
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With no content in 'content' or 'otherwise'"
          }
        }
      ]
    },
    {
      "@type": "stencila:ForBlock",
      "stencila:code": {
        "string": "[]"
      },
      "stencila:variable": "item",
      "stencila:content": [],
      "stencila:otherwise": []
    }
  ]
}