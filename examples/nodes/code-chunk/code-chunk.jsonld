{
  "@context": {
    "@vocab": "https://schema.org/",
    "stencila": "https://stencila.org/"
  },
  "@type": "Article",
  "stencila:content": [
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With no language:"
          }
        }
      ]
    },
    {
      "@type": "stencila:CodeChunk",
      "stencila:code": {
        "string": "No language"
      }
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With language:"
          }
        }
      ]
    },
    {
      "@type": "stencila:CodeChunk",
      "stencila:code": {
        "string": "# Some python code\na = 3"
      },
      "programmingLanguage": "python"
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With language and execution mode:"
          }
        }
      ]
    },
    {
      "@type": "stencila:CodeChunk",
      "stencila:executionMode": "Always",
      "stencila:code": {
        "string": "// Javascript"
      },
      "programmingLanguage": "js"
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "With label:"
          }
        }
      ]
    },
    {
      "@type": "stencila:CodeChunk",
      "stencila:code": {
        "string": "# Chunk 1A"
      },
      "stencila:label": "1A",
      "stencila:labelAutomatically": false
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "Code figure with caption:"
          }
        }
      ]
    },
    {
      "@type": "stencila:CodeChunk",
      "stencila:code": {
        "string": "plot(y~x)"
      },
      "programmingLanguage": "r",
      "stencila:labelType": "FigureLabel",
      "stencila:label": "1",
      "stencila:labelAutomatically": false,
      "caption": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "Y against X."
              }
            }
          ]
        }
      ]
    },
    {
      "@type": "stencila:Paragraph",
      "stencila:content": [
        {
          "@type": "Text",
          "value": {
            "string": "Code table with caption:"
          }
        }
      ]
    },
    {
      "@type": "stencila:CodeChunk",
      "stencila:code": {
        "string": "head(mtcars)"
      },
      "programmingLanguage": "r",
      "stencila:labelType": "TableLabel",
      "stencila:label": "1",
      "stencila:labelAutomatically": false,
      "caption": [
        {
          "@type": "stencila:Paragraph",
          "stencila:content": [
            {
              "@type": "Text",
              "value": {
                "string": "Some cars."
              }
            }
          ]
        }
      ]
    }
  ]
}