{
  "$schema": "https://stencila.dev/Article.schema.json",
  "type": "Article",
  "content": [
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "An image "
        },
        {
          "type": "ImageObject",
          "contentUrl": "https://example.org/cat.jpg"
        },
        {
          "type": "Text",
          "value": ", with a caption "
        },
        {
          "type": "ImageObject",
          "contentUrl": "https://example.org/cat.jpg",
          "caption": [
            {
              "type": "Text",
              "value": "A cat"
            }
          ]
        },
        {
          "type": "Text",
          "value": ", with a caption and title "
        },
        {
          "type": "ImageObject",
          "contentUrl": "https://example.org/cat.jpg",
          "caption": [
            {
              "type": "Text",
              "value": "A cat"
            }
          ],
          "title": [
            {
              "type": "Text",
              "value": "The cat"
            }
          ]
        }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "Some audio "
        },
        {
          "type": "AudioObject",
          "contentUrl": "https://example.org/cat.mp3"
        }
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        {
          "type": "Text",
          "value": "A video "
        },
        {
          "type": "VideoObject",
          "contentUrl": "https://example.org/cat.mp4"
        }
      ]
    }
  ]
}
