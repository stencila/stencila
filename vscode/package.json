{
  "name": "stencila",
  "displayName": "Stencila",
  "publisher": "stencila",
  "description": "Documents with Scientific Intelligence",
  "version": "0.0.18",
  "license": "Apache-2.0",
  "author": "Stencila and contributors",
  "homepage": "https://github.com/stencila/stencila/tree/main/vscode#readme",
  "bugs": {
    "url": "https://github.com/stencila/stencila/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stencila/stencila",
    "directory": "vscode"
  },
  "icon": "icons/stencila-128.png",
  "engines": {
    "vscode": "^1.84.0"
  },
  "extensionDependencies": [
    "usernamehw.errorlens"
  ],
  "categories": [
    "AI",
    "Data Science",
    "Education",
    "Visualization",
    "Notebooks"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "authentication": [
      {
        "id": "stencila",
        "label": "Stencila Cloud"
      }
    ],
    "configuration": {
      "title": "Stencila",
      "properties": {
        "stencila.user.name": {
          "order": 1,
          "type": "string",
          "default": "",
          "description": "Your full name (optional). Used when attributing document changes."
        },
        "stencila.user.affiliations": {
          "order": 2,
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Your professional affiliations (optional). You can add multiple affiliations. Used when attributing document changes."
        },
        "stencila.user.object": {
          "order": 3,
          "description": "Advanced. A https://schema.org/Person object representing you, used when attributing document changes. Overrides other `user` settings above.",
          "type": [
            "object",
            "null"
          ],
          "default": null,
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "const": "Person"
            },
            "givenNames": {
              "type": "string"
            },
            "familyNames": {
              "type": "string"
            },
            "affiliations": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "type",
                  "name"
                ],
                "properties": {
                  "type": {
                    "const": "Organization"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "stencila.gutter.enabled": {
          "order": 4,
          "description": "Enable gutter with markers for document node type and status.",
          "type": "boolean",
          "default": true
        },
        "stencila.gutter.markerWidth": {
          "order": 5,
          "description": "Width of document node type gutter markers.",
          "type": "number",
          "minimum": 1,
          "maximum": 6,
          "default": 4
        },
        "stencila.gutter.animateRunning": {
          "order": 6,
          "description": "Animate gutter marker for running document nodes.",
          "type": "boolean",
          "default": true
        },
        "stencila.linkedScrolling.previewFollowsEditor": {
          "order": 7,
          "description": "Synchronize the scroll position of the preview to the scroll position of the editor.",
          "type": "boolean",
          "default": true
        },
        "stencila.linkedScrolling.editorFollowsPreview": {
          "order": 8,
          "description": "Synchronize the scroll position of the editor to the scroll position of the preview.",
          "type": "boolean",
          "default": true
        },
        "stencila.languageServer.logLevel": {
          "order": 9,
          "description": "The logging level of the Stencila Language Server (server restart required)",
          "enum": [
            "trace",
            "debug",
            "info",
            "warn",
            "error"
          ],
          "default": "info"
        }
      }
    },
    "commands": [
      {
        "command": "stencila.new-smd",
        "category": "Create",
        "title": "New Stencila Markdown document",
        "shortTitle": "Stencila Markdown",
        "icon": "$(new-file)"
      },
      {
        "command": "stencila.new-myst",
        "category": "Create",
        "title": "New MyST Markdown document",
        "shortTitle": "MyST Markdown",
        "icon": "$(new-file)"
      },
      {
        "command": "stencila.new-qmd",
        "category": "Create",
        "title": "New Quarto Markdown document",
        "shortTitle": "Quarto Markdown",
        "icon": "$(new-file)"
      },
      {
        "command": "stencila.new-chat",
        "category": "Create",
        "title": "New Stencila AI Chat",
        "shortTitle": "Chat",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "stencila.new-prompt",
        "category": "Create",
        "title": "New Stencila AI Prompt",
        "shortTitle": "Prompt",
        "icon": "$(comment)"
      },
      {
        "command": "stencila.view-doc",
        "category": "Stencila",
        "title": "Preview document",
        "shortTitle": "Preview doc",
        "icon": "$(preview)"
      },
      {
        "command": "stencila.chat-doc",
        "category": "Stencila",
        "title": "Chat about document",
        "shortTitle": "Document chat",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "stencila.insert-chat",
        "category": "Stencila",
        "title": "Start a chat",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "stencila.insert-chat-create",
        "category": "Stencila",
        "title": "Chat to create"
      },
      {
        "command": "stencila.insert-chat-edit",
        "category": "Stencila",
        "title": "Chat to edit"
      },
      {
        "command": "stencila.insert-chat-fix",
        "category": "Stencila",
        "title": "Chat to fix"
      },
      {
        "command": "stencila.insert-command-create",
        "category": "Stencila",
        "title": "Command to create",
        "icon": "$(sparkle)"
      },
      {
        "command": "stencila.insert-command-edit",
        "category": "Stencila",
        "title": "Command to edit",
        "icon": "$(pencil)"
      },
      {
        "command": "stencila.insert-command-fix",
        "category": "Stencila",
        "title": "Command to fix",
        "icon": "$(wrench)"
      },
      {
        "command": "stencila.invoke.run-curr",
        "category": "Stencila",
        "title": "Run this node",
        "shortTitle": "Run node",
        "icon": "$(run)"
      },
      {
        "command": "stencila.invoke.run-above",
        "category": "Stencila",
        "title": "Run nodes above and then this node",
        "shortTitle": "Run above",
        "icon": "$(run-above)"
      },
      {
        "command": "stencila.invoke.run-below",
        "category": "Stencila",
        "title": "Run this node and then nodes below",
        "shortTitle": "Run below",
        "icon": "$(run-below)"
      },
      {
        "command": "stencila.invoke.run-doc",
        "category": "Stencila",
        "title": "Run document",
        "shortTitle": "Run doc",
        "icon": "$(run-all)"
      },
      {
        "command": "stencila.invoke.run-code",
        "category": "Stencila",
        "title": "Run all code in document",
        "shortTitle": "Run code",
        "icon": "$(zap)"
      },
      {
        "command": "stencila.invoke.run-instruct",
        "category": "Stencila",
        "title": "Run all commands in document",
        "shortTitle": "Run instructions",
        "icon": "$(sparkle)"
      },
      {
        "command": "stencila.invoke.cancel-curr",
        "category": "Stencila",
        "title": "Cancel running this node",
        "icon": "$(stop)"
      },
      {
        "command": "stencila.invoke.cancel-doc",
        "category": "Stencila",
        "title": "Cancel all running nodes in the document",
        "shortTitle": "Cancel all",
        "icon": "$(stop)"
      },
      {
        "command": "stencila.invoke.lock-curr",
        "category": "Stencila",
        "title": "Lock this node",
        "shortTitle": "Lock node",
        "icon": "$(lock)"
      },
      {
        "command": "stencila.invoke.unlock-curr",
        "category": "Stencila",
        "title": "Unlock this node",
        "shortTitle": "Unlock node",
        "icon": "$(unlock)"
      },
      {
        "command": "stencila.invoke.prev-node",
        "category": "Stencila",
        "title": "Go to previous suggestion",
        "shortTitle": "Previous suggestion",
        "icon": "$(arrow-left)"
      },
      {
        "command": "stencila.invoke.next-node",
        "category": "Stencila",
        "title": "Go to next suggestion",
        "shortTitle": "Next suggestion",
        "icon": "$(arrow-right)"
      },
      {
        "command": "stencila.invoke.retry-node",
        "category": "Stencila",
        "title": "Retry the command by creating a new suggestion",
        "shortTitle": "Retry command",
        "icon": "$(refresh)"
      },
      {
        "command": "stencila.invoke.revise-node",
        "category": "Stencila",
        "title": "Revise the current suggestion based on feedback",
        "shortTitle": "Revise suggestion",
        "icon": "$(refresh)"
      },
      {
        "command": "stencila.invoke.archive-node",
        "category": "Stencila",
        "title": "Accept current suggestion and archive command",
        "shortTitle": "Accept suggestion",
        "icon": "$(archive)"
      },
      {
        "command": "stencila.invoke.save-doc",
        "category": "Stencila",
        "title": "Save document with sidecar file",
        "shortTitle": "Save doc",
        "icon": "$(save-all)"
      },
      {
        "command": "stencila.invoke.export-doc",
        "category": "Stencila",
        "title": "Export document",
        "shortTitle": "Export doc",
        "icon": "$(export)"
      },
      {
        "command": "stencila.setup.refresh",
        "category": "Stencila",
        "title": "Refresh setup checklist",
        "shortTitle": "Refresh setup list",
        "icon": "$(refresh)"
      },
      {
        "command": "stencila.kernels.refresh",
        "category": "Stencila",
        "title": "Refresh kernels list",
        "shortTitle": "Refresh kernels",
        "icon": "$(refresh)"
      },
      {
        "command": "stencila.kernels.use",
        "category": "Stencila",
        "title": "Use kernel in document",
        "shortTitle": "Use kernel",
        "icon": "$(arrow-circle-right)"
      },
      {
        "command": "stencila.prompts.picker",
        "category": "Stencila",
        "title": "Prompt pricker",
        "shortTitle": "Find and use a prompt",
        "icon": "$(filter)"
      },
      {
        "command": "stencila.prompts.refresh",
        "category": "Stencila",
        "title": "Refresh prompts list",
        "shortTitle": "Refresh prompts",
        "icon": "$(refresh)"
      },
      {
        "command": "stencila.prompts.open",
        "category": "Stencila",
        "title": "Open prompt in editor",
        "shortTitle": "Open prompt",
        "icon": "$(go-to-file)"
      },
      {
        "command": "stencila.prompts.use",
        "category": "Stencila",
        "title": "Use prompt in document",
        "shortTitle": "Use prompt",
        "icon": "$(arrow-circle-right)"
      },
      {
        "command": "stencila.models.refresh",
        "category": "Stencila",
        "title": "Refresh models list",
        "shortTitle": "Refresh models",
        "icon": "$(refresh)"
      },
      {
        "command": "stencila.models.use",
        "category": "Stencila",
        "title": "Use model for current command",
        "shortTitle": "Use model",
        "icon": "$(arrow-circle-right)"
      },
      {
        "command": "stencila.secrets.set",
        "category": "Stencila",
        "title": "Set secret",
        "icon": "$(workspace-trusted)"
      },
      {
        "command": "stencila.secrets.delete",
        "category": "Stencila",
        "title": "Delete secret",
        "icon": "$(workspace-untrusted)"
      },
      {
        "command": "stencila.lsp-server.restart",
        "category": "Stencila",
        "title": "Restart Language Server"
      },
      {
        "command": "stencila.lsp-server.logs",
        "category": "Stencila",
        "title": "Language Server Logs"
      },
      {
        "command": "stencila.walkthroughs.expand",
        "category": "Stencila",
        "title": "Expand current walkthrough",
        "shortTitle": "Expand walkthrough",
        "icon": "$(chevron-down)"
      },
      {
        "command": "stencila.walkthroughs.collapse",
        "category": "Stencila",
        "title": "Collapse and restart current walkthrough",
        "shortTitle": "Collapse and restart walkthrough",
        "icon": "$(chevron-up)"
      },
      {
        "command": "stencila.cloud.signin",
        "category": "Stencila Cloud",
        "title": "Sign in",
        "icon": "$(sign-in)"
      },
      {
        "command": "stencila.cloud.signout",
        "category": "Stencila Cloud",
        "title": "Sign out",
        "icon": "$(sign-out)"
      },
      {
        "command": "stencila.settings",
        "category": "Stencila",
        "title": "Settings",
        "icon": "$(gear)"
      },
      {
        "command": "stencila.shell",
        "category": "Stencila",
        "title": "Open a shell with the Stencila CLI available",
        "shortTitle": "Stencila Shell",
        "icon": "$(terminal)"
      }
    ],
    "keybindings": [
      {
        "command": "stencila.command-picker",
        "key": "ctrl+f1",
        "mac": "cmd+f1"
      },
      {
        "command": "stencila.view-doc",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.chat-doc",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.insert-chat",
        "key": "ctrl+/",
        "mac": "cmd+/",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.export-doc",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.run-curr",
        "key": "ctrl+enter",
        "mac": "cmd+enter",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.run-above",
        "key": "ctrl+1 enter",
        "mac": "cmd+1 enter",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.run-below",
        "key": "ctrl+2 enter",
        "mac": "cmd+2 enter",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.run-doc",
        "key": "ctrl+shift+enter",
        "mac": "cmd+shift+enter",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.prev-node",
        "key": "ctrl+alt+[",
        "mac": "cmd+alt+[",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.next-node",
        "key": "ctrl+alt+]",
        "mac": "cmd+alt+]",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.archive-node",
        "key": "ctrl+alt+\\",
        "mac": "cmd+alt+\\",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.retry-node",
        "key": "ctrl+r",
        "mac": "cmd+r",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.invoke.revise-node",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      },
      {
        "command": "stencila.prompts.picker",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|myst|qmd)$/"
      }
    ],
    "menus": {
      "file/newFile": [
        {
          "command": "stencila.new-smd",
          "group": "Document"
        },
        {
          "command": "stencila.new-myst",
          "group": "Document"
        },
        {
          "command": "stencila.new-qmd",
          "group": "Document"
        },
        {
          "command": "stencila.new-chat",
          "group": "AI"
        },
        {
          "command": "stencila.new-prompt",
          "group": "AI"
        }
      ],
      "commandPalette": [
        {
          "command": "stencila.shell"
        },
        {
          "command": "stencila.new-smd"
        },
        {
          "command": "stencila.new-myst"
        },
        {
          "command": "stencila.new-qmd"
        },
        {
          "command": "stencila.new-chat"
        },
        {
          "command": "stencila.new-prompt"
        },
        {
          "command": "stencila.view-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.chat-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.run-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.run-above",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.run-below",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.run-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.run-code",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.run-instruct",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.cancel-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.cancel-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.lock-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.unlock-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.save-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.invoke.export-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.walkthroughs.expand",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        },
        {
          "command": "stencila.walkthroughs.collapse",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/"
        }
      ],
      "editor/title/run": [
        {
          "command": "stencila.view-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@1"
        },
        {
          "command": "stencila.invoke.run-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@2"
        },
        {
          "command": "stencila.invoke.export-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@3"
        },
        {
          "command": "stencila.chat-doc",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@4"
        }
      ],
      "editor/context": [
        {
          "command": "stencila.insert-chat",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@1"
        },
        {
          "command": "stencila.insert-chat-create",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@2"
        },
        {
          "command": "stencila.insert-chat-edit",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@3"
        },
        {
          "command": "stencila.insert-chat-fix",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "000-Stencila@4"
        },
        {
          "command": "stencila.invoke.run-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "001-Stencila@1"
        },
        {
          "command": "stencila.invoke.lock-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "001-Stencila@2"
        },
        {
          "command": "stencila.invoke.unlock-curr",
          "when": "editorLangId =~ /^(md|smd|myst|qmd)$/",
          "group": "001-Stencila@3"
        }
      ],
      "view/title": [
        {
          "command": "stencila.setup.refresh",
          "when": "view == stencila-setup",
          "group": "navigation"
        },
        {
          "command": "stencila.prompts.picker",
          "when": "view == stencila-prompts",
          "group": "navigation"
        },
        {
          "command": "stencila.prompts.refresh",
          "when": "view == stencila-prompts",
          "group": "navigation"
        },
        {
          "command": "stencila.models.refresh",
          "when": "view == stencila-models",
          "group": "navigation"
        },
        {
          "command": "stencila.kernels.refresh",
          "when": "view == stencila-kernels",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "stencila.cloud.signin",
          "when": "view == stencila-setup && viewItem == stencila-cloud-signed-out",
          "group": "inline"
        },
        {
          "command": "stencila.cloud.signout",
          "when": "view == stencila-setup && viewItem == stencila-cloud-signed-in",
          "group": "inline"
        },
        {
          "command": "stencila.secrets.set",
          "when": "view == stencila-setup && viewItem == ai-api-key-none",
          "group": "inline"
        },
        {
          "command": "stencila.secrets.delete",
          "when": "view == stencila-setup && viewItem == ai-api-key-some",
          "group": "inline"
        },
        {
          "command": "stencila.settings",
          "when": "view == stencila-setup && viewItem == user-details",
          "group": "inline"
        },
        {
          "command": "stencila.kernels.use",
          "when": "view == stencila-kernels && viewItem == kernel",
          "group": "inline"
        },
        {
          "command": "stencila.prompts.open",
          "when": "view == stencila-prompts && viewItem == prompt",
          "group": "inline"
        },
        {
          "command": "stencila.prompts.use",
          "when": "view == stencila-prompts && viewItem == prompt",
          "group": "inline"
        },
        {
          "command": "stencila.cloud.signin",
          "when": "view == stencila-models && viewItem == signin",
          "group": "inline"
        },
        {
          "command": "stencila.models.use",
          "when": "view == stencila-models && viewItem == model",
          "group": "inline"
        }
      ]
    },
