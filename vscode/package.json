{
  "name": "stencila",
  "displayName": "Stencila",
  "publisher": "stencila",
  "description": "Documents with Scientific Intelligence",
  "version": "2.9.0",
  "license": "Apache-2.0",
  "author": "Stencila and contributors",
  "homepage": "https://github.com/stencila/stencila/tree/main/vscode#readme",
  "bugs": {
    "url": "https://github.com/stencila/stencila/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stencila/stencila",
    "directory": "vscode"
  },
  "icon": "icons/stencila-128.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "extensionDependencies": [
    "ms-python.python",
    "usernamehw.errorlens"
  ],
  "categories": [
    "AI",
    "Data Science",
    "Education",
    "Visualization",
    "Notebooks"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "authentication": [
      {
        "id": "stencila",
        "label": "Stencila Cloud"
      }
    ],
    "configuration": {
      "title": "Stencila",
      "properties": {
        "stencila.user.name": {
          "order": 1,
          "type": "string",
          "default": "",
          "description": "Your full name (optional). Used when attributing document changes."
        },
        "stencila.user.affiliations": {
          "order": 2,
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [],
          "description": "Your professional affiliations (optional). You can add multiple affiliations. Used when attributing document changes."
        },
        "stencila.user.object": {
          "order": 3,
          "description": "Advanced. A https://schema.org/Person object representing you, used when attributing document changes. Overrides other `user` settings above.",
          "type": [
            "object",
            "null"
          ],
          "default": null,
          "required": [
            "type"
          ],
          "properties": {
            "type": {
              "const": "Person"
            },
            "givenNames": {
              "type": "string"
            },
            "familyNames": {
              "type": "string"
            },
            "affiliations": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "type",
                  "name"
                ],
                "properties": {
                  "type": {
                    "const": "Organization"
                  },
                  "name": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "stencila.documents.onSave": {
          "order": 15,
          "description": "What action to take when a document is saved in the editor",
          "enumDescriptions": [
            "Do nothing",
            "If the document is already being tracked, store it in the tracking directory",
            "If the document is not yet tracked track it, and then store it in the tracking directory"
          ],
          "enum": [
            "Nothing",
            "Store",
            "Track"
          ],
          "default": "Store"
        },
        "stencila.diagnostics.compilationMessages": {
          "order": 21,
          "description": "The minimum severity level of compilation messages (from code linting, math parsing, etc) to show in editor",
          "enum": [
            "Trace",
            "Debug",
            "Info",
            "Warning",
            "Error",
            "Exception"
          ],
          "default": "Error"
        },
        "stencila.diagnostics.executionMessages": {
          "order": 22,
          "description": "The minimum severity level of execution messages (from runtime logging and exceptions) to show in editor",
          "enum": [
            "Trace",
            "Debug",
            "Info",
            "Warning",
            "Error",
            "Exception"
          ],
          "default": "Warning"
        },
        "stencila.gutter.enabled": {
          "order": 30,
          "description": "Enable gutter with markers for document node type and status.",
          "type": "boolean",
          "default": true
        },
        "stencila.gutter.markerWidth": {
          "order": 31,
          "description": "Width of document node type gutter markers.",
          "type": "number",
          "minimum": 1,
          "maximum": 6,
          "default": 4
        },
        "stencila.gutter.animateRunning": {
          "order": 32,
          "description": "Animate gutter marker for running document nodes.",
          "type": "boolean",
          "default": true
        },
        "stencila.linkedScrolling.previewFollowsEditor": {
          "order": 60,
          "description": "Synchronize the scroll position of the preview to the scroll position of the editor.",
          "type": "boolean",
          "default": true
        },
        "stencila.linkedScrolling.editorFollowsPreview": {
          "order": 61,
          "description": "Synchronize the scroll position of the editor to the scroll position of the preview.",
          "type": "boolean",
          "default": true
        },
        "stencila.languageServer.logLevel": {
          "order": 100,
          "description": "The logging level of the Stencila Language Server (server restart required)",
          "enum": [
            "trace",
            "debug",
            "info",
            "warn",
            "error"
          ],
          "default": "info"
        },
        "stencila.logging.usage": {
          "order": 130,
          "description": "Enable logging of extension usage events. Helps us fix bugs and improve usability.",
          "type": "boolean",
          "default": true,
          "tags": [
            "telemetry",
            "usesOnlineServices"
          ]
        }
      }
    },
