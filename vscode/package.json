      },
      {
        "command": "stencila.invoke.archive-node",
        "key": "ctrl+alt+\\",
        "mac": "cmd+alt+\\",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|qmd|myst|latex)$/"
      },
      {
        "command": "stencila.invoke.retry-node",
        "key": "ctrl+r",
        "mac": "cmd+r",
        "when": "editorTextFocus && editorLangId =~ /^(md|smd|qmd|myst|latex)$/"
      },
