        {
          "@id": "stencila:CodeExecutable"
        },
        {
          "@id": "stencila:CodeStatic"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:Instruction"
        },
        {
          "@id": "stencila:Math"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:QuoteBlock"
        },
        {
          "@id": "stencila:Section"
