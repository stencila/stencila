      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:label",
      "@type": "rdfs:Property",
      "rdfs:label": "label",
      "rdfs:comment": "A short label for the math block.",
      "schema:domainIncludes": [
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "schema:Table"
        },
        {
          "@id": "stencila:Button"
        },
        {
          "@id": "stencila:CodeChunk"
        },
        {
          "@id": "stencila:Figure"
        },
        {
          "@id": "stencila:MathBlock"
