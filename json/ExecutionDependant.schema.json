{
  "$schema": "https://stencila.org/meta.schema.json",
  "$id": "https://stencila.org/ExecutionDependant.schema.json",
  "@id": "stencila:ExecutionDependant",
  "title": "ExecutionDependant",
  "nick": "exd",
  "extends": [
    "Entity"
  ],
  "category": "flow",
  "description": "A downstream execution dependant of a node.",
  "required": [
    "type",
    "dependantRelation",
    "dependantType",
    "dependantId"
  ],
  "core": [
    "id",
    "codeLocation"
  ],
  "properties": {
    "type": {
      "@id": "schema:type",
      "description": "The type of this item.",
      "$comment": "This is a special property analogous to JSON-LD's `@type` keyword.\n",
      "type": "string"
    },
    "id": {
      "@id": "schema:id",
      "description": "The identifier for this item.",
      "$comment": "This is a special property analogous to JSON-LD's `@id` keyword.\n",
      "strip": [
        "metadata"
      ],
      "html": {
        "attr": "id"
      },
      "type": "string"
    },
    "dependantRelation": {
      "@id": "stencila:dependantRelation",
      "description": "The relation to the dependant.",
      "aliases": [
        "dependant-relation",
        "dependant_relation"
      ],
      "$ref": "ExecutionDependantRelation.schema.json"
    },
    "dependantType": {
      "@id": "stencila:dependantType",
      "description": "The type of node that is the dependant.",
      "aliases": [
        "dependant-type",
        "dependant_type"
      ],
      "type": "string"
    },
    "dependantId": {
      "@id": "stencila:dependantId",
      "description": "The id of node that is the dependant.",
      "aliases": [
        "dependant-id",
        "dependant_id"
      ],
      "type": "string"
    },
    "codeLocation": {
      "@id": "stencila:codeLocation",
      "description": "The location that the dependant is defined.",
      "aliases": [
        "code-location",
        "code_location"
      ],
      "$ref": "CodeLocation.schema.json"
    }
