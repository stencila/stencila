{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "stencila:TableCell",
      "@type": "rdfs:Class",
      "rdfs:label": "TableCell",
      "rdfs:comment": "A cell within a `Table`.",
      "rdfs:subClassOf": {
        "@id": "Entity"
      }
    },
    {
      "@id": "schema:name",
      "@type": "rdfs:Property",
      "rdfs:label": "name",
      "rdfs:comment": "The name of the cell.",
      "schema:domainIncludes": [
        {
          "@id": "schema:Thing"
        },
        {
          "@id": "stencila:Button"
        },
        {
          "@id": "stencila:ExecutionTag"
        },
        {
          "@id": "stencila:Function"
        },
        {
          "@id": "stencila:Parameter"
        },
        {
          "@id": "stencila:TableCell"
        },
        {
          "@id": "stencila:Variable"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:colspan",
      "@type": "rdfs:Property",
      "rdfs:label": "colspan",
      "rdfs:comment": "How many columns the cell extends.\n",
      "schema:domainIncludes": {
        "@id": "stencila:TableCell"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Number"
      }
    },
    {
      "@id": "stencila:cellType",
      "@type": "rdfs:Property",
      "rdfs:label": "cellType",
      "rdfs:comment": "The type of cell.",
      "schema:domainIncludes": {
        "@id": "stencila:TableCell"
      }
    },
    {
      "@id": "stencila:rowspan",
      "@type": "rdfs:Property",
      "rdfs:label": "rowspan",
      "rdfs:comment": "How many columns the cell extends.",
      "schema:domainIncludes": {
        "@id": "stencila:TableCell"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Number"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "Contents of the table cell.",
      "schema:domainIncludes": [
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "schema:CreativeWork"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Division"
        },
        {
          "@id": "stencila:For"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:IfClause"
        },
        {
          "@id": "stencila:Include"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:QuoteBlock"
        },
        {
          "@id": "stencila:Span"
        },
        {
          "@id": "stencila:TableCell"
        }
      ],
      "schema:rangeIncludes": [
        {
          "@id": "stencila:Block"
        },
        {
          "@id": "stencila:Inline"
        }
      ]
    }
  ]
}