{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "stencila:Division",
      "@type": "rdfs:Class",
      "rdfs:label": "Division",
      "rdfs:comment": "Styled block content.",
      "rdfs:subClassOf": {
        "@id": "stencila:Styled"
      }
    },
    {
      "@id": "stencila:code",
      "@type": "rdfs:Property",
      "rdfs:label": "code",
      "rdfs:comment": "The code of the equation in the `styleLanguage`.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:CallArgument"
        },
        {
          "@id": "stencila:CodeExecutable"
        },
        {
          "@id": "stencila:CodeStatic"
        },
        {
          "@id": "stencila:Math"
        },
        {
          "@id": "stencila:Styled"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:Cord"
      }
    },
    {
      "@id": "stencila:styleLanguage",
      "@type": "rdfs:Property",
      "rdfs:label": "styleLanguage",
      "rdfs:comment": "The language used for the style specification e.g. css, tw",
      "schema:domainIncludes": {
        "@id": "stencila:Styled"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:compilationDigest",
      "@type": "rdfs:Property",
      "rdfs:label": "compilationDigest",
      "rdfs:comment": "A digest of the `code` and `styleLanguage`.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Executable"
        },
        {
          "@id": "stencila:Math"
        },
        {
          "@id": "stencila:Styled"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:CompilationDigest"
      }
    },
    {
      "@id": "stencila:compilationErrors",
      "@type": "rdfs:Property",
      "rdfs:label": "compilationErrors",
      "rdfs:comment": "Errors generated when parsing and transpiling the style.",
      "schema:domainIncludes": [
        {
          "@id": "stencila:Executable"
        },
        {
          "@id": "stencila:Math"
        },
        {
          "@id": "stencila:Styled"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:CompilationError"
      }
    },
    {
      "@id": "stencila:css",
      "@type": "rdfs:Property",
      "rdfs:label": "css",
      "rdfs:comment": "A Cascading Style Sheet (CSS) transpiled from the `code` property.",
      "schema:domainIncludes": {
        "@id": "stencila:Styled"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:classes",
      "@type": "rdfs:Property",
      "rdfs:label": "classes",
      "rdfs:comment": "A list of class names associated with the node.",
      "schema:domainIncludes": {
        "@id": "stencila:Styled"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "The content within the division",
      "schema:domainIncludes": [
        {
          "@id": "schema:Article"
        },
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "schema:Comment"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Admonition"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Division"
        },
        {
          "@id": "stencila:Figure"
        },
        {
          "@id": "stencila:For"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:IfClause"
        },
        {
          "@id": "stencila:Include"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:QuoteBlock"
        },
        {
          "@id": "stencila:Section"
        },
        {
          "@id": "stencila:Span"
        },
        {
          "@id": "stencila:Suggestion"
        },
        {
          "@id": "stencila:TableCell"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:Block"
      }
    }
  ]
}