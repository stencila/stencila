{
  "@context": {
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "schema": "https://schema.org/",
    "stencila": "https://stencila.dev/"
  },
  "@graph": [
    {
      "@id": "stencila:Section",
      "@type": "rdfs:Class",
      "rdfs:label": "Section",
      "rdfs:comment": "A section of a document.",
      "rdfs:subClassOf": {
        "@id": "stencila:Entity"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "The content within the section.",
      "schema:domainIncludes": [
        {
          "@id": "schema:Article"
        },
        {
          "@id": "schema:Claim"
        },
        {
          "@id": "schema:Comment"
        },
        {
          "@id": "schema:ListItem"
        },
        {
          "@id": "stencila:Admonition"
        },
        {
          "@id": "stencila:Cite"
        },
        {
          "@id": "stencila:Figure"
        },
        {
          "@id": "stencila:ForBlock"
        },
        {
          "@id": "stencila:Form"
        },
        {
          "@id": "stencila:Heading"
        },
        {
          "@id": "stencila:IfBlockClause"
        },
        {
          "@id": "stencila:IncludeBlock"
        },
        {
          "@id": "stencila:Link"
        },
        {
          "@id": "stencila:Mark"
        },
        {
          "@id": "stencila:Note"
        },
        {
          "@id": "stencila:Paragraph"
        },
        {
          "@id": "stencila:QuoteBlock"
        },
        {
          "@id": "stencila:Section"
        },
        {
          "@id": "stencila:StyledBlock"
        },
        {
          "@id": "stencila:StyledInline"
        },
        {
          "@id": "stencila:SuggestionBlock"
        },
        {
          "@id": "stencila:SuggestionInline"
        },
        {
          "@id": "stencila:TableCell"
        }
      ],
      "schema:rangeIncludes": {
        "@id": "stencila:Block"
      }
    },
    {
      "@id": "stencila:sectionType",
      "@type": "rdfs:Property",
      "rdfs:label": "sectionType",
      "rdfs:comment": "The type of section.",
      "schema:domainIncludes": {
        "@id": "stencila:Section"
      },
      "schema:rangeIncludes": {
        "@id": "stencila:SectionType"
      }
    }
  ]
}