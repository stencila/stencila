        "@id": "stencila:Citation"
      },
      "schema:rangeIncludes": {
        "@id": "stencila:CitationIntent"
      }
    },
    {
      "@id": "stencila:content",
      "@type": "rdfs:Property",
      "rdfs:label": "content",
      "rdfs:comment": "A rendering of the citation using the citation style of the document.",
