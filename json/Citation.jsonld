        "@id": "stencila:Citation"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    },
    {
      "@id": "stencila:citationSuffix",
      "@type": "rdfs:Property",
      "rdfs:label": "citationSuffix",
      "rdfs:comment": "Text to show after the citation.",
      "schema:domainIncludes": {
        "@id": "stencila:Citation"
      },
      "schema:rangeIncludes": {
        "@id": "schema:Text"
      }
    }
