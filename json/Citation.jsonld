        {
          "@id": "schema:Article"
        },
        {
          "@id": "schema:PublicationIssue"
        },
        {
          "@id": "schema:PublicationVolume"
        },
        {
          "@id": "stencila:Citation"
        },
        {
          "@id": "stencila:Reference"
